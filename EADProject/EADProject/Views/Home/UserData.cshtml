@{
    Layout = "_Layout";
    ViewData["Title"] = "BlogSpot/Home";
}
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />

</head>
<style>

    body {
        color: #fff;
        background: white;
    }

    .table {
        margin-left: auto;
        margin-right: auto;
        overflow-x: auto;
        border: 2px solid black;
        width: 40%;
    }

    .head {
        text-align: center;
        color: #19aa8d;
    }
</style>
<div class="main">
    <h2 class="head">Welcome to Home Page</h2>
    <table class="table">
        <tr>
            <th>UserName</th>
            <th>Email</th>
        </tr>
        @foreach (var x in ViewBag.x)
        {
            <tr>
                <td>@x.UserName</td>
                <td>@x.Email</td>
            </tr>
        }
    </table>
</div>
